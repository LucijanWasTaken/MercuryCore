{
  "name": "mercury",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "format": "biome format src --write",
    "lint": "biome lint src",
    "prod": "bun run lines && bun i && vite build",
    "preview": "vite preview",
    "buildview": "vite build && vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lines": "scc -c --no-size --no-cocomo --sort lines --exclude-dir staging --exclude-dir setup --exclude-dir PrepForUpload  --exclude-file mercury.yml,uno.config.ts,AppSettings.xml,ReflectionMetadata.xml,materials.json,shaders.json --exclude-ext lua,h,txt --ci -f cloc-yaml -o ./data/lines"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.4",
    "@melt-ui/pp": "^0.3.0",
    "@melt-ui/svelte": "^0.76.3",
    "@sveltejs/adapter-node": "^5.0.1",
    "@sveltejs/kit": "^2.5.5",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@types/bun": "^1.0.12",
    "@types/d3-interpolate": "^3.0.4",
    "@types/nprogress": "^0.2.3",
    "@types/serviceworker": "^0.0.84",
    "@types/three": "^0.163.0",
    "@unocss/core": "^0.59.0-beta.1",
    "@unocss/extractor-svelte": "^0.59.0-beta.1",
    "@unocss/preset-tagify": "^0.59.0-beta.1",
    "@unocss/transformer-directives": "^0.59.0-beta.1",
    "@unocss/transformer-variant-group": "^0.59.0-beta.1",
    "stylus": "^0.63.0",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.9",
    "svelte-inview": "^4.0.2",
    "svelte-parallax": "^0.6.0",
    "svelte-preprocess": "^5.1.3",
    "sveltekit-autoimport": "^1.8.0",
    "sveltekit-superforms": "^2.12.2",
    "unocss": "^0.59.0-beta.1",
    "zod": "^3.22.4"
  },
  "type": "module",
  "dependencies": {
    "@codemirror/commands": "^6.3.3",
    "@codemirror/lang-css": "^6.2.1",
    "@codemirror/state": "^6.4.1",
    "@codemirror/view": "^6.26.1",
    "@threlte/core": "7.2.1",
    "@threlte/extras": "^8.11.2",
    "codemirror": "^6.0.1",
    "custom-protocol-check": "^1.4.0",
    "d3-interpolate": "^3.0.1",
    "dotenv": "^16.4.5",
    "lucia": "^3.1.1",
    "nprogress": "^0.2.0",
    "oslo": "^1.2.0",
    "picocolors": "^1.0.0",
    "profane-words": "^1.5.11",
    "sharp": "^0.33.3",
    "stripe-gradient": "^1.0.1",
    "surrealdb.js": "^0.11.0",
    "three": "^0.163.0",
    "vite": "^5.2.8",
    "vite-plugin-warmup": "^0.1.0"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "svelte-preprocess"
  ]
}
