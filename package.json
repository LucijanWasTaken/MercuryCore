{
  "name": "mercury",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "format": "biome format src --write",
    "lint": "biome lint src",
    "prod": "bun run lines && bun i && vite build",
    "preview": "vite preview",
    "buildview": "vite build && vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lines": "scc -c --no-size --no-cocomo --sort lines --exclude-dir staging --exclude-dir setup --exclude-dir PrepForUpload  --exclude-file defs.d.lua,deno.lock,pnpm-lock.yaml,mercury.yml,uno.config.ts,AppSettings.xml,ReflectionMetadata.xml,materials.json,shaders.json --exclude-ext lua,h,txt --ci -f cloc-yaml -o ./data/lines"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.3",
    "@melt-ui/pp": "^0.3.0",
    "@melt-ui/svelte": "^0.76.2",
    "@sveltejs/adapter-node": "^5.0.1",
    "@sveltejs/kit": "^2.5.5",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@types/bun": "^1.0.12",
    "@types/d3-interpolate": "^3.0.4",
    "@types/nprogress": "^0.2.3",
    "@types/serviceworker": "^0.0.84",
    "@types/three": "^0.163.0",
    "@types/uuid": "^9.0.8",
    "@unocss/core": "^0.58.8",
    "@unocss/extractor-svelte": "^0.58.8",
    "@unocss/preset-tagify": "^0.58.8",
    "@unocss/transformer-directives": "^0.58.8",
    "@unocss/transformer-variant-group": "^0.58.8",
    "esbuild": "^0.20.2",
    "stylus": "^0.63.0",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.8",
    "svelte-inview": "^4.0.2",
    "svelte-parallax": "^0.6.0",
    "svelte-preprocess": "^5.1.3",
    "sveltekit-autoimport": "^1.8.0",
    "sveltekit-superforms": "^2.12.2",
    "typescript": "^5.4.3",
    "unocss": "^0.58.8",
    "zod": "^3.22.4"
  },
  "type": "module",
  "dependencies": {
    "@threlte/core": "7.2.1",
    "@threlte/extras": "8.11.1",
    "check-disk-space": "^3.4.0",
    "custom-protocol-check": "^1.4.0",
    "d3-interpolate": "^3.0.1",
    "dotenv": "^16.4.5",
    "lucia": "^3.1.1",
    "nprogress": "^0.2.0",
    "oslo": "^1.2.0",
    "picocolors": "^1.0.0",
    "profane-words": "^1.5.11",
    "sharp": "^0.33.3",
    "stripe-gradient": "^1.0.1",
    "surrealdb.js": "^0.11.0",
    "three": "^0.163.0",
    "vite": "^5.2.7",
    "vite-plugin-warmup": "^0.1.0"
  }
